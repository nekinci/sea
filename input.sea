package main


const string HELLO_WORLD = "Hello, world!"

struct Printer {
    i64 times
    string value
}

impl Printer {

    fun void print_value() {
        for var i64 i = 0; i < this.times; i++;
            println(this.value)
    }

}


fun i32 main() {
    var Printer* printer = new(Printer)
    *printer = Printer{times: 5, value: "HELLO_WORLD"}
    printer.print_value()
    return 0
}


